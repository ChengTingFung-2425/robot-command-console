{
  "name": "robot-command-console",
  "version": "1.0.0",
  "description": "本專案（robot-command-console）是一個用於機器人指令管理、路由與執行的整合式控制台與服務平台。",
  "scripts": {
    "start": "cd electron-app && npm start",
    "start:dev": "cd electron-app && npm run start:dev",
    "build": "cd electron-app && npm run build",
    "build:appimage": "cd electron-app && npm run build:appimage",
    "test": "python3 -m pytest tests/ -v",
    "test:integration": "node test_integration.js",
    "install:electron": "cd electron-app && npm install"
  },
  "keywords": [
    "robot",
    "command",
    "console",
    "electron",
    "flask"
  ],
  "author": "",
  "license": "ISC"
}
